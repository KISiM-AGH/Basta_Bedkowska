// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Word {
  id          Int    @id @default(autoincrement())
  word        String @unique
  description String
  category    String
}

model GameState {
  id        Int    @id @default(autoincrement())
  mistakes  String
  gameState String
  word      String
  wordID    Int
}
